#include<stdio.h>

int main() 
{
    int a[3][3][3], sum = 0;
    for(int i = 0; i<3; i++)
    {
        for(int j=0 ; j<3; j++)
        {
            for(int k=0; k<3; k++)
            {
                scanf("%d",&a[i][j][k]);
                sum = sum + a[i][j][k];
            }
        }
    }
    printf("Sum is : %d",sum);
}
